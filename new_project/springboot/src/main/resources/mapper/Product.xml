<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.newproduct.springboot.mapper.ProductMapper">

    <update id="update">
        update product
        <set>
            <if test="url != null">
                url = #{url},
            </if>
            <if test="sku != null">
                sku = #{sku},
            </if>
            <if test="sku13 != null">
                sku13 = #{sku13}
            </if>
        </set>
        <where>
            id = #{id}
        </where>
    </update>




</mapper>
